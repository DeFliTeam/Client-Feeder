
[Unit]
Description=defli-feed
Wants=network.target
After=network.target

[Service]
User=defli
RuntimeDirectory=defli-feed
ExecStart=/usr/local/share/defli/defli-feed.sh
Type=simple
Restart=always
RestartSec=30
StartLimitInterval=1
StartLimitBurst=100
SyslogIdentifier=defli-feed
Nice=-1

[Install]
WantedBy=default.target
